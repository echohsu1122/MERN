{
  "version": 2,
  "builds": [{ "src": "*.js", "use": "@vercel/node" }],
  "rewrites": [
    {
      "source": "/proxy/:match*",
      "destination": "https://mern-bice-eight.vercel.app/:match*"
    }
  ],
  "routes": [
    {
      "src": "/[^.]+",
      "dest": "/",
      "status": 200
    }
  ]
}
